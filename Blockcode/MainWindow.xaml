<Window x:Class="Blockcode.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Blockcode"
        mc:Ignorable="d"
        Title="Block Code" Height="600" Width="1000">
    <Grid>
        <DockPanel>
            <local:BlocksTab x:Name="BlocksTab" />
            <local:ScriptTab x:Name="ScriptTab" />
            <local:OutputTab x:Name="OutputTab" />
        </DockPanel>
    </Grid>
</Window>